{
  "name": "Zectrack",
  "version": "1.0",
  "description": "Zectrack 是一個免費的嘖嘖專案分析神器，讓你輕鬆追蹤專案歷史紀錄，分析集資金額與贊助人數掌握專案進度。",
  "permissions": ["activeTab", "declarativeContent", "storage", "tabs"],
  "content_scripts": [
    {
      "matches": ["https://www.zeczec.com/projects/*"],
      // "css": ["myStyles.css"],
      "js": ["contentScript.js"]
    }
  ],
  // "options_page": "options.html",
  // "background": {
  //   "scripts": ["background.js"],
  //   "persistent": false
  // },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": "zectrack-icon.png"
  },
  "icons": {
    "16": "zectrack-icon.png",
    "32": "zectrack-icon.png",
    "48": "zectrack-icon.png",
    "128": "zectrack-icon.png"
  },
  "manifest_version": 2
}
